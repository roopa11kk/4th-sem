n = int(input())  # input number of elements in the list
numbers = list(map(int, input().split()))  # input the list of numbers
max_num = numbers[0]
min_num = numbers[0]
for num in numbers:
    if num > max_num:
        max_num = num
    if num < min_num:
        min_num = num
print('max:',max_num)
print('min:',min_num)
